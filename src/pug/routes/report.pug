extends ../layout

block title
    | #{titles.report.text}

block header
    - md_header = {mode: 'default', text: titles.report.text, number: titles.report.number}
    - menus.lg_bottom.report.current = true
    include ../layout/header

block main
    main(class="asgar--container w-full mx-auto px-5 md:px-6 sm:px-5 xs:px-3 flex-grow")
        - title = titles.report
        include ../partials/h1

block footer
    - menus.md_bottom.report.current = true
    include ../layout/footer